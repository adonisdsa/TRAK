{
  "name": "trak-website",
  "private": true,
  "description": "",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "build": "GATSBY_ENV=production gatsby build",
    "build-docs": "(cd ../docsv3 && yarn build) && rm -rf public/docs && cp -R ../docsv3/build public/docs/",
    "build-docs-sdk": "(cd ../docs-sdk && yarn build) && rm -rf public/docs/api && cp -R ../docs-sdk/build public/docs/api",
    "build-sdk": "(cd ../sdk && yarn build) && rm -rf public/docs/api && mkdir -p public/docs/api && cp -R ../sdk/out/ public/docs/api/",
    "build-all": "yarn build && yarn build-docs && yarn build-sdk",
    "develop": "yarn format && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "GATSBY_ENV=local npm run develop",
    "serve": "GATSBY_ENV=local gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "yarn clean && yarn build-all && yarn deploy-to-s3 && yarn deploy-clear-cache",
    "deploy-to-s3": "AWS_PROFILE='INSERT_NAME_HERE' AWS_REGION=ap-southeast-2 gatsby-plugin-s3 deploy -y",
    "deploy-clear-cache": "AWS_PROFILE='INSERT_NAME_HERE' aws cloudfront create-invalidation --distribution-id 'INSERT_ID' --paths \"/*\""
  },
  "dependencies": {
    "@aaronhayes/react-use-hubspot-form": "^1.1.0",
